
<%- include('../partials/header') %>

            <main class="main">
               <%- include('../partials/search') %>
                
                

                <div class="main-list">
                    <h1>LAST MOVIES: </h1>
                        
                        <ol class="main-list-ol">
                            <div class="list-container">
                                <% movies.forEach(movie => { %>
                                    <a href="/movies/detail/<%= movie.id %>"><li class="main-list-li"><i class="far fa-check-circle"></i><%= movie.title %><span class="number-release_date"><%= moment(movie.release_date).format('DD-MM-YYYY') %></i></span>
                                        <button>DETAIL</button></li></a>
                                <% }) %>
                            </div>    
                        </ol>


                </div>    
            </main>

        </div>
            
    </div>

</body>
</html>